<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.techgadgets">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_gadget"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_gadget_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TechGadgets">
        <activity android:name=".mouse"></activity>
        <activity android:name=".hdmi" />
        <activity android:name=".usb" />
        <activity android:name=".monitor" />
        <activity android:name=".headphones" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>